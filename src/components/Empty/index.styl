.Empty 
    width 100%
    height 100%
    display flex
    position absolute
    justify-content center
    align-items center
    font-size 16px
    flex-direction column
    padding-bottom 2%
    top 0
    pointer-events none

    &-inner
        opacity .5
    
    img 
        display block
        margin 0 auto 10px
        width 32px
